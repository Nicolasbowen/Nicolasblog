<%- include header %>
<ul class="archive">
    <% var lastYear = 0 %>
    <% posts.forEach(function (post, index) { %>
    <% if (lastYear != post.time.month) { %>
    <li><h3><%= post.time.month %></h3></li>
    <% lastYear = post.time.month } %>
    <li><time><%= post.time.minute %></time></li>
    <li><a href="/u/<%= post.name %>/<%= post.time.minute %>/<%= post.title %>"><%= post.title %></a></li>
    <% }) %>
</ul>
<%- include footer %>