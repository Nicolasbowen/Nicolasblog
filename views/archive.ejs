<%- include header %>
<div class="arch">
<form action="" class="form">
<ul class="archive">
    <% var lastMoth = 0 %>
    <% posts.forEach(function (post, index) { %>
    <% if (lastMoth != post.time.month) { %>
    <li><h3><%= post.time.month %></h3></li>
    <% lastMoth=post.time.month } %>
    <li><time><%= post.time.day %></time></li>
    <li><a href="/u/<%= post.name %>/<%= post.time.day %>/<%=  post.title %>"><%= post.title %></a></li>
    <% }) %>
</ul>
</form>
</div>
<%- include footer %>